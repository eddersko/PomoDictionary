<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/elenor"
        android:layout_below="@+id/edna"
        android:text="Elenor Stevenson Gonzales"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/elenor"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_above="@+id/textView1"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="35dp"
        android:gravity="left"
        android:scaleType="centerCrop" />

    <ImageView
        android:id="@+id/edna"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="70dp"
        android:layout_toRightOf="@+id/elenor"
        android:gravity="right"
        android:scaleType="centerCrop" />

  <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="45dp"
        android:layout_below="@+id/edna"
        android:text="Edna Campbell Guerrero"
        android:textSize="20sp" />
    <WebView
    	    android:id="@+id/webView1"
    	    android:layout_width="500dp"
    	    android:layout_height="350dp"
    	    android:layout_alignLeft="@+id/textView1"
    	    android:layout_alignRight="@+id/edna"
    	    android:layout_below="@+id/textView1"
    	    android:layout_marginTop="35dp" />

 </RelativeLayout>
